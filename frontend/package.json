{
  "name": "frontend",
  "version": "1.0.0",
  "author": "Triton Software Engineering <tse@ucsd.edu>",
  "private": true,
  "dependencies": {
    "@material-table/core": "^2.3.26",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "node --max_old_space_size=4096 node_modules/.bin/react-scripts start",
    "build": "node --max_old_space_size=4096 node_modules/.bin/react-scripts build",
    "lint": "eslint --fix --cache --report-unused-disable-directives . && prettier --write .",
    "format": "prettier --write .",
    "watch": "cra-build-watch -b ../backend/public",
    "lint-check": "eslint --cache --report-unused-disable-directives . && prettier --check .",
    "prepare": "cd .. && husky install .husky"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.13.14",
    "cra-build-watch": "^3.4.0",
    "eslint": "^7.23.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-react": "^7.23.1",
    "husky": "^6.0.0",
    "prettier": "^2.2.1"
  }
}
